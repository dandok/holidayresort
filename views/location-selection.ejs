          <div class="menu-btn">
            <a href="#" class="brand">Holiday Resort</a>
          </div>
          <!-- <div class="navigation"> -->
          <div class="navigation-items">
            <a href="/holidayresort">Home</a>
            <a href="/about">About</a>
